{
  "name": "@crimson-witnesses/socials",
  "version": "2025.08.27",
  "main": "src/pages/index.tsx",
  "author": "Drake Costa <drake@saeris.io> (https://saeris.gg)",
  "license": "MIT",
  "private": true,
  "repository": "git@github.com:crimson-witnesses/socials.git",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "yarn lint --fix",
    "lint": "eslint \"./src/**/*.{j,t}s?(x)\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "framer-motion": "^12.23.12",
    "next": "^15.5.2",
    "next-pwa": "^5.6.0",
    "ogl": "^1.0.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@saeris/eslint-config": "^3.1.2",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.8",
    "eslint": "^9.34.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.6.2",
    "typescript": "5.9.2",
    "typescript-plugin-css-modules": "^5.2.0"
  },
  "packageManager": "yarn@4.9.3",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "postcss": {
    "plugins": [
      "postcss-nested",
      [
        "postcss-preset-env",
        {
          "autoprefixer": {
            "flexbox": "no-2009"
          },
          "features": {
            "oklab-function": {
              "preserve": true
            },
            "relative-color-syntax": {
              "preserve": true
            }
          }
        }
      ]
    ]
  }
}
